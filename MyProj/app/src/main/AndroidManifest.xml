        </activity>
        <activity android:label="Main 엑티비티" android:name=".MainActivity"/>
        <activity android:label="Second 엑티비티" android:name=".SecondActivity"/>
        <activity android:label="Third 엑티비티" android:name=".ThirdActivity"/>
        <activity android:label="signup 엑티비티" android:name=".SignupActivity"/>

    </application>

</manifest>